{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.EntityFrameworkCore": "Information",
      "OrderService": "Debug"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=MyStore_Orders_Dev;Username=postgres;Password=dev_password",
    "Redis": "localhost:6379"
  },
  "JwtSettings": {
    "SecretKey": "development-jwt-secret-key-for-testing-only-32-chars-minimum",
    "Issuer": "MyStore.OrderService.Dev",
    "Audience": "MyStore.Client.Dev",
    "AccessTokenExpiryMinutes": 60,
    "RefreshTokenExpiryDays": 30
  },
  "RabbitMQ": {
    "Host": "localhost",
    "Port": 5672,
    "Username": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "ExchangeName": "MyStore.Orders.Dev",
    "QueueName": "order-events-dev"
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "http://localhost:4200",
      "https://localhost:7000",
      "http://localhost:8080"
    ],
    "AllowedMethods": [ "*" ],
    "AllowedHeaders": [ "*" ],
    "AllowCredentials": true
  },
  "Cache": {
    "DefaultExpirationMinutes": 1,
    "UserOrdersExpirationMinutes": 1
  }
}
